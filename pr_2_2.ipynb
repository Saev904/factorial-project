   "source": [
    "def factorial(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)\n",
    "\n",
    "for i in range(5):\n",
    "    print(f\"Facorial {i} = {factorial(i)}\")\n",
    "\n",
    "number = int(input(\"Number = \"))\n",
    "print(f\"Factorial {number} = {factorial(number)}\")"
   ]
